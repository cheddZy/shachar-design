<style>

    ul {
        padding: 0.5rem;
        display: grid;
        gap: 0.5rem;
        grid-template-columns: 1fr 1fr 1fr 1fr;
    }

    @media screen and (max-width: 1300px) {
        ul {
            grid-template-columns: 1fr 1fr 1fr;
        }
    }

    @media screen and (max-width: 1000px) {
        ul {
            grid-template-columns: 1fr 1fr 1fr;
        }
    }

    @media screen and (max-width: 768px) {
        ul {
            grid-template-columns: 1fr 1fr;
        }
    }

</style>